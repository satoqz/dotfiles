{
  "breadcrumbs.enabled": false,
  "catppuccin-icons.hidesExplorerArrows": true,
  "catppuccin-icons.specificFolders": false,
  "catppuccin.accentColor": "blue",
  "catppuccin.extraBordersEnabled": true,
  "catppuccin.italicComments": false,
  "catppuccin.italicKeywords": false,
  "catppuccin.workbenchMode": "flat",
  "diffEditor.renderGutterMenu": false,
  "editor.accessibilitySupport": "off",
  "editor.codeLens": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.folding": false,
  "editor.fontFamily": "Iosevka",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlayHints.fontSize": 12,
  "editor.inlayHints.padding": true,
  "editor.lightbulb.enabled": "off",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": "singleFile",
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": "off",
  "errorLens.borderRadius": "0.5em",
  "errorLens.fontSize": "12",
  "errorLens.messageBackgroundMode": "message",
  "errorLens.padding": "0.3em",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.defaultCloneDirectory": "~/src",
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "git.rebaseWhenSync": true,
  "git.replaceTagsWhenPull": true,
  "git.showCommitInput": false,
  "markdown.preview.fontFamily": "Iosevka",
  "markdown.preview.fontSize": 16,
  "markdown.preview.markEditorSelection": false,
  "problems.decorations.enabled": false,
  "remote.SSH.remotePlatform": {
    "*.satoqz.net": "linux"
  },
  "scm.diffDecorationsGutterPattern": {
    "added": true,
    "modified": true
  },
  "scm.diffDecorationsGutterWidth": 2,
  "search.decorations.badges": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "Ghostty.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "none",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.env.osx": {
    "EDITOR": "code --wait"
  },
  "terminal.integrated.fontFamily": "Iosevka Term",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.shellIntegration.showCommandGuide": false,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "terminal.integrated.tabs.enabled": false,
  "vim.highlightedyank.enable": true,
  "vim.useSystemClipboard": true,
  "window.commandCenter": false,
  "window.newWindowDimensions": "inherit",
  "window.title": "${activeEditorMedium}",
  "workbench.activityBar.location": "default",
  "workbench.colorCustomizations": {
    "[Catppuccin Frappé]": {
      "editorGroup.border": "#0000",
      "editorOverviewRuler.background": "#0000",
      "tab.activeBorderTop": "#0000",
      "tab.selectedBorderTop": "#0000",
      "tab.unfocusedActiveBorderTop": "#0000",
      "tab.unfocusedInactiveBackground": "#0000"
    }
  },
  "workbench.colorTheme": "Catppuccin Frappé",
  "workbench.editor.decorations.badges": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "catppuccin-frappe",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "feather-vscode",
  "workbench.reduceMotion": "on",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none",
  "zig.path": "zig",
  "zig.zls.enabled": "on",
  "zig.zls.path": "zls"
}
