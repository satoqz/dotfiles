{
  "assistant": {
    "button": false,
    "default_model": {
      "model": "claude-3-5-sonnet-latest",
      "provider": "zed.dev"
    },
    "enabled": false,
    "version": "2"
  },
  "buffer_font_family": "Iosevka",
  "buffer_font_size": 18,
  "buffer_line_height": {
    "custom": 1.5
  },
  "collaboration_panel": {
    "button": false
  },
  "cursor_blink": false,
  "features": {
    "inline_completion_provider": "none"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "gutter": {
    "code_actions": false,
    "folds": false
  },
  "indent_guides": {
    "enabled": false
  },
  "notification_panel": {
    "button": false
  },
  "project_panel": {
    "indent_guides": {
      "show": "never"
    },
    "indent_size": 10,
    "show_diagnostics": "off"
  },
  "scrollbar": {
    "show": "system"
  },
  "show_user_picture": false,
  "ssh_connections": [
    {
      "host": "orb",
      "nickname": "OrbStack",
      "projects": []
    }
  ],
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "cursor_shape": "bar",
    "env": {
      "EDITOR": "zed --wait"
    },
    "font_family": "Iosevka Term",
    "line_height": {
      "custom": 1.4
    },
    "option_as_meta": true,
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "theme": "Xcode High Contrast Darker",
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": false
  },
  "ui_font_family": "Iosevka",
  "ui_font_size": 18,
  "vim_mode": true
}
