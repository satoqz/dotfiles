{
  "theme": "Xcode Classic Darker",
  "vim_mode": true,
  "buffer_font_family": "Iosevka",
  "buffer_line_height": { "custom": 1.4 },
  "ui_font_family": "Iosevka",
  "cursor_blink": false,
  "pane_split_direction_vertical": "right",
  "pane_split_direction_horizontal": "down",
  "scrollbar": { "show": "system", "axes": { "horizontal": false } },
  "show_user_picture": false,
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "toolbar": { "quick_actions": false },
  "git": { "inline_blame": { "enabled": false } },
  "gutter": { "code_actions": false, "folds": false },
  "indent_guides": { "enabled": false },
  "inlay_hints": { "show_parameter_hints": false },
  "project_panel": {
    "indent_guides": { "show": "never" },
    "indent_size": 8,
    "show_diagnostics": "off"
  },
  "terminal": {
    "cursor_shape": "bar",
    "env": { "EDITOR": "zed --wait" },
    "font_family": "Iosevka Term",
    "line_height": { "custom": 1.4 },
    "option_as_meta": true,
    "toolbar": { "breadcrumbs": false }
  },
  "features": { "edit_prediction_provider": "none" },
  "assistant": {
    "version": "2",
    "default_model": {
      "model": "claude-3-5-sonnet-latest",
      "provider": "zed.dev"
    }
  },
  "ssh_connections": [
    { "host": "orb", "nickname": "OrbStack", "projects": [] }
  ],
  "languages": { "Git Commit": { "wrap_guides": [50, 72] } },
  "telemetry": { "diagnostics": false, "metrics": false }
}
